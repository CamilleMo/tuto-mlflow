name: basic_project

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      learning_rate: {type: float, default: 0.1}
      number_of_epochs: {type: integer, default: 10}
      intercept: {type: string, default: false}
    command: "python train.py {learning_rate} {number_of_epochs} {intercept}"